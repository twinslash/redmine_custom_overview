<% [:wiki, :wall, :roadmap].each do |permission| %>
  <%= render :partial => permission.to_s if User.current.allowed_to?(:"custom_overview_#{permission}", @project) %>
<% end %>

<% content_for :sidebar do %>
  <% [:spent_time, :members, :latest_news].each do |permission| %>
    <%= render :partial => permission.to_s if User.current.allowed_to?(:"custom_overview_#{permission}", @project) %>
  <% end %>
<% end %>

